{
  "name": "cointokenhub/cryptocompare-api-php",
  "description": "cryptocompare-api-php is an API Wrapper for the cryptocompare API",
  "type": "library",
  "require": {
    "php": "^5.3.3 || ^7.0",
    "guzzlehttp/guzzle": "^6.3",
    "illuminate/support": "~5.4.0|~5.5.0|~5.6.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^4.8.35 || ^5.7 || ^7.1",
    "squizlabs/php_codesniffer": "^3.2"
  },
  "autoload": {
    "psr-4": {
      "CoinTokenHub\\CryptoCompareApi\\": "src/"
    }
  },
  "scripts": {
    "test": "./vendor/bin/phpunit"
  },
  "license": "MIT",
  "authors": [
    {
      "name": "Kailash Chandrasekaran",
      "email": "me@ckailash.com"
    }
  ],
  "extra": {
    "laravel": {
      "providers": [
        "CoinTokenHub\\CryptoCompareApi\\CryptoCompareServiceProvider"
      ],
      "aliases": {
        "CryptoCompare": "CoinTokenHub\\CryptoCompareApi\\CryptoCompareServiceFacade"
      }
    }
  }
}
